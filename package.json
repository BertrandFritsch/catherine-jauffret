{
  "private": true,
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "graphql": "16"
      },
      "ignoreMissing": [
        "@babel/core",
        "webpack",
        "sharp",
        "babel-eslint"
      ]
    },
    "packageExtensions": {
      "gatsby-plugin-utils": {
        "dependencies": {
          "fastq": "*"
        }
      },
      "babel-plugin-remove-graphql-queries": {
        "dependencies": {
          "@babel/types": "*"
        }
      },
      "gatsby-plugin-netlify": {
        "dependencies": {
          "webpack": "*"
        }
      },
      "gatsby-plugin-image": {
        "dependencies": {
          "semver": "*"
        }
      },
      "gatsby-plugin-page-creator": {
        "dependencies": {
          "fs-extra": "*"
        }
      }
    }
  }
}
